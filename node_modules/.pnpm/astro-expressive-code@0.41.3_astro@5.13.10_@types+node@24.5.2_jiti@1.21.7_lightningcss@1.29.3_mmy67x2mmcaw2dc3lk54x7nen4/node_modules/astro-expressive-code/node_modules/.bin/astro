#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/config/workspace/create_fuwari/node_modules/.pnpm/astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules/astro/node_modules:/config/workspace/create_fuwari/node_modules/.pnpm/astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules:/config/workspace/create_fuwari/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/config/workspace/create_fuwari/node_modules/.pnpm/astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules/astro/node_modules:/config/workspace/create_fuwari/node_modules/.pnpm/astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules:/config/workspace/create_fuwari/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@5.13.10_@types+node@24.5.2_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_st_fxyvhfmq545nmtyzmtwqzlc24y/node_modules/astro/astro.js" "$@"
fi
